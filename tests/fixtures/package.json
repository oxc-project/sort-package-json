{
  "version": "1.0.0",
  "name": "test-package",
  "$schema": "https://json.schemastore.org/package",
  "displayName": "Test Package Display",
  "description": "A comprehensive test package for sorting",
  "categories": ["testing", "utilities", "testing"],
  "keywords": ["test", "sorting", "json", "test", "example"],
  "homepage": "https://github.com/test/test-package#readme",
  "bugs": {
    "email": "bugs@example.com",
    "url": "https://github.com/test/test-package/issues"
  },
  "license": "MIT",
  "author": {
    "url": "https://example.com",
    "email": "author@example.com",
    "name": "Test Author"
  },
  "maintainers": [
    "Zebra Maintainer <zebra@example.com>",
    "Apple Maintainer <apple@example.com>",
    {
      "url": "https://maintainer1.com",
      "name": "Object Maintainer",
      "email": "maintainer1@example.com"
    }
  ],
  "contributors": [
    "Zebra Contributor <zebra@example.com>",
    {
      "name": "Zebra Person",
      "email": "zebra@example.com",
      "url": "https://zebra.example.com"
    },
    {
      "name": "Apple Person",
      "email": "apple@example.com",
      "url": "https://apple.example.com"
    }
  ],
  "repository": {
    "url": "https://github.com/test/test-package",
    "type": "git"
  },
  "funding": {
    "url": "https://github.com/sponsors/test",
    "type": "github"
  },
  "man": "./man/doc.1",
  "bin": {
    "cli": "./bin/cli.js",
    "app": "./bin/app.js"
  },
  "directories": {
    "doc": "./docs",
    "lib": "./lib",
    "test": "./tests",
    "bin": "./bin"
  },
  "files": [
    "src",
    "!src/internal",
    "dist",
    "!dist/temp",
    "dist",
    "README.md",
    "LICENSE",
    "bin/cli.js"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "browser": "./dist/browser.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js"
    },
    "./paths-should-keep-b_a": {
      "b": "./index.cjs",
      "a": "./index.js"
    },
    "./package.json": "./package.json",
    "./utils": {
      "import": "./dist/utils.esm.js",
      "default": "./dist/utils.js",
      "types": "./dist/utils.d.ts"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "posttest": "echo 'Tests complete'",
    "build": "webpack",
    "lint": "eslint .",
    "pretest": "echo 'Starting tests'",
    "dev": "webpack serve"
  },
  "dependencies": {
    "react": "^18.0.0",
    "axios": "^1.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "webpack": "^5.0.0",
    "jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=16.0.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.0.0"
  },
  "bundledDependencies": ["lodash", "axios"],
  "resolutions": {
    "lodash": "^4.17.21"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"]
  },
  "babel": {
    "presets": ["@babel/preset-env", "@babel/preset-react"],
    "plugins": ["@babel/plugin-proposal-class-properties"]
  },
  "eslintConfig": {
    "extends": ["eslint:recommended"],
    "env": {
      "node": true
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0"
  },
  "volta": {
    "node": "18.0.0"
  },
  "packageManager": "npm@8.0.0",
  "customField": "this is a custom unknown field",
  "anotherCustom": "another custom field",
  "_custom": "private field with underscore",
  "_id": "test-package@1.0.0",
  "_internal": "another private field"
}
